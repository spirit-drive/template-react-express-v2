{
  "parser": "babel-eslint",
  "extends": ["airbnb", "prettier"],
  "plugins": ["prettier"],
  "env": { "jest": true },
  "rules": {
  "prettier/prettier": ["error"],
    "import/no-unresolved": 0,
    "no-use-before-define": ["error", { "variables": false }],
    "react/forbid-prop-types": 0,
    "react/jsx-filename-extension": 0
  }
}
